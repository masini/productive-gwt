<!--
	<!DOCTYPE document SYSTEM
	"http://google-web-toolkit.googlecode.com/svn/releases/1.5/distro-source/core/src/gwt-module.dtd">
-->
<module>

	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />
   <inherits name='com.google.gwt.user.theme.standard.Standard'/>     

	<entry-point class='org.googlecode.reflection.client.GeneratorTestEntryPoint' />
	
	<generate-with class="org.googlecode.reflection.rebind.DataBindGenerator">
		<when-type-assignable class="org.googlecode.reflection.client.DataBindable" />
	</generate-with>	

</module>

